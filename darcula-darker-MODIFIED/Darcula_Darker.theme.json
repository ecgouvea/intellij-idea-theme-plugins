{
  "name": "Darcula Darker-MODIFIED",
  "dark": true,
  "author": "MODIFIED",
  "editorScheme": "/Darcula_Darker.xml",
  "ui": {
    "*": {
      "background": "#000000",
      "disabledBackground": "#000000",
      "inactiveBackground": "#000000",

      "selectionBackground": "#222222",
      "selectionInactiveBackground": "#111111",

      "lightSelectionBackground": "#001111",
      "lightSelectionInactiveBackground": "#000011",

      "borderColor": "#414245",
      "disabledBorderColor": "#333336",

      "separatorForeground": "#414245",
      "separatorColor": "#414245",
      "lineSeparatorColor": "#414245"
    },

    "Borders": {
      "color": "#414245",
      "ContrastBorderColor": "#414245"
    },

    "Tree.rowHeight": 20,

    "VersionControl": {
      "Log.Commit": {
        "currentBranchBackground": "#000000",
        "unmatchedForeground": "#727380"
      },
      "FileHistory.Commit.selectedBranchBackground": "#000000"
    },

    "DefaultTabs": {
      "hoverBackground": "#00000000",
      "underlineColor": "#2f65ca",
      "underlinedTabBackground": "#000000"
    },

    "SearchEverywhere": {
      "Tab": {
        "selectedForeground": "#CFD2D5",
        "selectedBackground": "#111111"
      }
    },

    "EditorTabs": {
      "selectedBackground": "#222222",
      "inactiveMaskColor": "#00000033",
      "underlinedTabBackground": "#000000",
      "underlinedTabForeground": "#CFD2D5"
    },

    "ToolWindow.HeaderTab": {
      "underlinedTabBackground": "#000000"
    }
  }
}